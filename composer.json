{
    "name": "hughcube/ynjspx",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "hugh.li",
            "email": "hugh.li@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "hughcube/purl": "*",
        "hughcube/guzzle-http": "*"
    },
    "require-dev": {
        "laravel/framework": "*",
        "laravel/lumen-framework": "*",
        "squizlabs/php_codesniffer": "*",
        "phpstan/phpstan": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4": {
            "HughCube\\Ynjspx\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HughCube\\Ynjspx\\Tests\\": "tests"
        }
    },
    "extra": {},
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse -vvv",
        "check-style": "phpcs -p --standard=PSR2 src/ -v",
        "fix-style": "phpcbf -p --standard=PSR2 src/ -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
